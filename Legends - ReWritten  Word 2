---Abyss Knight Level:370
local MobName = "AbyssKnight"
local BowAttack = true
local Mon = true

    Tab4:AddToggle({
        Name = "Abyss Knight Level:370",
        Default = false,
        Callback = function(value)
            Mon = value
            if Mon then
    
                local tweenService = game:GetService("TweenService")
                local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear)
                local tweenServiceClose = game:GetService("TweenService")
                local tweenInfoClose = TweenInfo.new(0.7, Enum.EasingStyle.Linear)
                local player = game:GetService("Players").LocalPlayer
                local char = player.Character

            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 130}),
            {CFrame = CFrame.new(4464.462890625, -681.4097900390625, 6409.998046875)})
            Tw:Play()
            wait(5)

           while Mon do
                for i,v in pairs(game.workspace.Mobs:GetChildren()) do
                    if string.match(v.Name, MobName) and v:FindFirstChild("HumanoidRootPart") then
                        if v.HumanoidRootPart and v.HumanoidRootPart:IsA("BasePart") and v.HumanoidRootPart.Position.Z - char.HumanoidRootPart.Position.Z <= 5000 then
                            BowAttack = true
                            spawn(function()
                                while BowAttack and v.Humanoid and v.Humanoid.Health > 0 do
                                    char.Bow.SwordScript.Shoot:FireServer(v.HumanoidRootPart.Position, 21)
                                    wait(0.1)
                                end
                            wait(30)
                            BowAttack = false
                            wait(5)
                            BowAttack = true
                            end)

                            repeat wait() until v.Humanoid.Health <= 0
                            BowAttack = false
                            wait(1)
                            local Tw = tweenService:Create(char.HumanoidRootPart, TweenInfo.new(2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 130}), 
                                    {CFrame = v.HumanoidRootPart.CFrame})
                                    Tw:Play()
                            wait(8)
                            local Tw2 = tweenService:Create(char.HumanoidRootPart, TweenInfo.new(2, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 130}),
                                    {CFrame = CFrame.new(4464.462890625, -681.4097900390625, 6409.998046875)})
                                    Tw2:Play()
                            wait(2)
                        end
                    end
                end
                wait()
            end
            Mon = false
        end
    end
})


