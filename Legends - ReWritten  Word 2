---Frenzy Ore
Tab2:AddToggle({Name = "Frenzy Ore",Default = false,Callback = function(value)
    Frenzy = value
        if value then
            local TweenService = game:GetService("TweenService")
            local Character = game.Players.LocalPlayer.Character
            local HumanoidRootPart = Character.HumanoidRootPart
        
        local Tween1 = TweenService:Create(HumanoidRootPart, TweenInfo.new(6, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 200}),
            {CFrame = CFrame.new(4814.251953125, -778.5182495117188, 6515.0166015625)})
        Tween1:Play()
        Tween1.Completed:Connect(function()
            TweenRunning = false
        end)

        Tween1.Completed:Connect(function()
        wait(2)
        spawn(function()
            while value and not TweenRunning do
                HumanoidRootPart.CFrame = CFrame.new(4793.74365234375, -783.8568725585938, 6475.79638671875)
            end
        end)
        
        local PickAxe = true  
        while PickAxe do wait()
            game:GetService("Players").LocalPlayer.Character.PickAxe.SwordScript.Activate:FireServer()
            end
    end)
else

    PickAxe = false
    TweenRunning = true
end
end
})

