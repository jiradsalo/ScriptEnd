local MobName = {"MarineBoat","MarineGun","MarineBazooka","MarineSword","Cannon1"
    }
local BowAttack = true
local Mon = true

    Tab3:AddToggle({
        Name = "All Raid Gigy",
        Default = false,
        Callback = function(value)
            Mon = value
            if Mon then
    
        local tweenService = game:GetService("TweenService")
        local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear)
        local tweenServiceClose = game:GetService("TweenService")
        local tweenInfoClose = TweenInfo.new(0.7, Enum.EasingStyle.Linear)
        local player = game:GetService("Players").LocalPlayer
        local char = player.Character

        while Mon do
            for i,v in pairs(game.workspace.Mobs:GetChildren()) do
                if table.find(MobName, v.Name) and v:FindFirstChild("HumanoidRootPart") and v.Health.Value > 0 then
                tweenServiceClose:Create(player.Character.HumanoidRootPart, tweenInfoClose, {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,10)}):Play()
                    

                 spawn(function()
                    while BowAttack do wait()
                    char.Bow.SwordScript.Shoot:FireServer(v.HumanoidRootPart.Position, 21)
                    end
                 end)
                wait(2)
                end
            end
            wait()
        end
    end
    Mon = false
end
})

