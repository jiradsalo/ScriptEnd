                HumanoidRootPart.CFrame = CFrame.new(4726.85595703125, -781.67236328125, 6652.6982421875)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(4807.01318359375, -781.0653686523438, 6607.42724609375)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(4827.76220703125, -773.6183471679688, 6457.05517578125)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(4279.275390625, -808.8141479492188, 6411.01171875)
                wait(3)
            end
        end)
        
        _G.loop = true
        while _G.loop do
            wait()
            workspace.cgg250za.PickAxe.SwordScript.Activate:FireServer()
            wait(0.1)
        end
    end)
else

    _G.loop = false
    TweenRunning = true
end
end
})

------------------------------------------------------------------------------------------------------------------------------------

---Frenzy Ore
Tab2:AddToggle({Name = "Frenzy Ore",Default = false,Callback = function(value)
    Frenzy = value
        if value then
            local TweenService = game:GetService("TweenService")
            local Character = game.Players.LocalPlayer.Character
            local HumanoidRootPart = Character.HumanoidRootPart
        
        local Tween1 = TweenService:Create(HumanoidRootPart, TweenInfo.new(6, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 200}),
            {CFrame = CFrame.new(4830.4248046875, -773.9840698242188, 6471.947265625)})
        Tween1:Play()
        Tween1.Completed:Connect(function()
            TweenRunning = false
        end)

        Tween1.Completed:Connect(function()
        wait(2)
        spawn(function()
            while value and not TweenRunning do
                HumanoidRootPart.CFrame = CFrame.new(4799.3095703125, -786.069580078125, 6477.14697265625)
                wait(3)
            end
        end)



        _G.loop = true
        while _G.loop do
            wait()
            workspace.cgg250za.PickAxe.SwordScript.Activate:FireServer()
            wait(0.1)
        end
    end)
else

    _G.loop = false
    TweenRunning = true
end
end
})

