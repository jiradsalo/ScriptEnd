Tab2:AddToggle({Name = "Bronz Ore",Default = false,Callback = function(value)
    Bronz = value
        if value then
            local TweenService = game:GetService("TweenService")
            local Character = game.Players.LocalPlayer.Character
            local HumanoidRootPart = Character.HumanoidRootPart

            local Tween1 = TweenService:Create(HumanoidRootPart, TweenInfo.new(6, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 200}),
                {CFrame = CFrame.new(248.7452850341797, 51.284996032714844, -338.2414855957031)})
            Tween1:Play()
            Tween1.Completed:Connect(function()
                TweenRunning = false
            end)

            Tween1.Completed:Connect(function()
                wait(2)
                spawn(function()
                    while value and not TweenRunning do
                        HumanoidRootPart.CFrame = CFrame.new(239.96347045898438, 52.77890396118164, -315.93853759765625)
                        wait(3)
                        HumanoidRootPart.CFrame = CFrame.new(194.9040985107422, 19.517311096191406, -337.9369201660156)
                        wait(3)
                        HumanoidRootPart.CFrame = CFrame.new(187.6529083251953, 19.407312393188477, -392.93359375)
                        wait(3)
                        HumanoidRootPart.CFrame = CFrame.new(287.47027587890625, 37.73102951049805, -320.20965576171875)
                        wait(3)
                    end
                end)

                _G.loop = true
                while _G.loop do
                    wait()
                    game:GetService("Players").LocalPlayer.Character.PickAxe.SwordScript.Activate:FireServer()
                    wait(0.1)
                end
            end)
        else

            _G.loop = false
            TweenRunning = true
        end
end
})

