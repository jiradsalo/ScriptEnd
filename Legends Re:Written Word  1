---Kaze Level:100
local MobName = "Kaze"
local BowAttack = true
local Mon = true

    Tab4:AddToggle({
        Name = "Kaze Level:100",
        Default = false,
        Callback = function(value)
            Mon = value
            if Mon then
    
                local tweenService = game:GetService("TweenService")
                local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear)
                local tweenServiceClose = game:GetService("TweenService")
                local tweenInfoClose = TweenInfo.new(0.7, Enum.EasingStyle.Linear)
                local player = game:GetService("Players").LocalPlayer
                local char = player.Character
                
                local distance = (CFrame.new(-3393.783447265625, -339.6742248535156, -1576.4881591796875).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                local speed = 300
                local time = distance / speed
                local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                    {CFrame = CFrame.new(-3393.783447265625, -339.6742248535156, -1576.4881591796875)})
                Tw:Play()
                wait(5)

           while Mon do
                for i,v in pairs(game.workspace.Mobs:GetChildren()) do
                    if string.match(v.Name, MobName) and v:FindFirstChild("HumanoidRootPart") then
                        if v.HumanoidRootPart and v.HumanoidRootPart:IsA("BasePart") and v.HumanoidRootPart.Position.Z - char.HumanoidRootPart.Position.Z <= 5000 then

                            BowAttack = true
                            spawn(function()
                                while BowAttack and v.Humanoid and v.Humanoid.Health > 0 do
                                    char.Bow.SwordScript.Shoot:FireServer(v.HumanoidRootPart.Position, 21)
                                    wait(0.1)
                                end
                            wait(30)
                            BowAttack = false
                            wait(5)
                            BowAttack = true
                            end)

                             repeat wait() until v.Humanoid.Health <= 0
                            BowAttack = false
                            wait(1)
                            local distance = (v.HumanoidRootPart.CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = v.HumanoidRootPart.CFrame})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(-3046.301513671875, -336.8670654296875, -1549.7684326171875).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(-3046.301513671875, -336.8670654296875, -1549.7684326171875)})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(-3393.783447265625, -339.6742248535156, -1576.4881591796875).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(-3393.783447265625, -339.6742248535156, -1576.4881591796875)})
                            Tw:Play()
                            wait(2)
                        end
                    end
                end
