            local HumanoidRootPart = Character.HumanoidRootPart
        
        local Tween1 = TweenService:Create(HumanoidRootPart, TweenInfo.new(6, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 200}),
            {CFrame = CFrame.new(-3174.155029296875, 421.57598876953125, -532.0110473632812)})
        Tween1:Play()
        Tween1.Completed:Connect(function()
            TweenRunning = false
        end)

        Tween1.Completed:Connect(function()
        wait(2)
        spawn(function()
            while value and not TweenRunning do
                HumanoidRootPart.CFrame = CFrame.new(-3181.39453125, 421.57598876953125, -562.011474609375)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-3509.86328125, 438.9498596191406, -676.9973754882812)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-3239.185546875, 376.0263671875, -1150.364501953125)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-3236.302978515625, 260.0061950683594, -1423.3037109375)
                wait(3)
            end
        end)
        
        _G.loop = true
        while _G.loop do
            wait()
            game:GetService("Players").LocalPlayer.Character.PickAxe.SwordScript.Activate:FireServer()
            wait(0.1)
        end
    end)
else

    _G.loop = false
    TweenRunning = true
end
end
})

