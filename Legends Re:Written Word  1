---DualSwords Melioofus Level:200
local MobName = "Melioofus"
local BowAttack = false
local Mon = false

local MelioofusMon = true
Tab4:AddToggle({
    Name = "Melioofus Level:200",
    Default = false,
    Callback = function(value)
            Mon = value
            if Mon then
    
    
            local tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)
            local tweenServiceClose, tweenInfoClose = game:GetService("TweenService"), TweenInfo.new(0.7, Enum.EasingStyle.Linear)
            local player = game:GetService("Players").LocalPlayer
            local char = player.Character
                
            local distance = (CFrame.new(-1004.2633056640625, 339.01983642578125, 175.7955322265625).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
            local speed = 300
            local time = distance / speed
            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                {CFrame = CFrame.new(-1004.2633056640625, 339.01983642578125, 175.7955322265625)})
            Tw:Play()
            wait(5)
            
            while Mon do
                for i,v in pairs(game.workspace.Mobs:GetChildren()) do
                    if string.match(v.Name, MobName) and v:FindFirstChild("HumanoidRootPart") then
                        if v.HumanoidRootPart and v.HumanoidRootPart:IsA("BasePart") and v.HumanoidRootPart.Position.Z - char.HumanoidRootPart.Position.Z <= 5000 then

                            spawn(function()
                                BowAttack = true
                                while BowAttack and v.Humanoid and v.Humanoid.Health > 0 do wait()
                                    char.DualSwords.SwordScript.Activate:FireServer()
                                end
                                wait(30)
                                BowAttack = false
                                wait(5)
                                BowAttack = true
                            end)
                            

                            while Mon and v.Humanoid.Health > 0 do
                                wait(1)
                                tweenServiceClose:Create(player.Character.HumanoidRootPart, tweenInfoClose, {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,3)}):Play()
                            end
             
                              repeat wait() until v.Humanoid.Health <= 0
                            BowAttack = false
                            wait(1)
                            local distance = (v.HumanoidRootPart.CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = v.HumanoidRootPart.CFrame})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(256.6656799316406, 893.0773315429688, 2806.488525390625).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(256.6656799316406, 893.0773315429688, 2806.488525390625)})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(-1004.2633056640625, 339.01983642578125, 175.7955322265625).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(-1004.2633056640625, 339.01983642578125, 175.7955322265625)})
                            Tw:Play()
                            wait(2)
                        end
                    end
                end
                wait()
            end
            Mon = false
        end
    end
