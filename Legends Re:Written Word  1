---DualSwords Rouge Paladin Level:80
local MobName = "RougePaladin"
local BowAttack = false
local Mon = false

    local RougePaladinMob = true
    Tab4:AddToggle({
        Name = "Rouge Paladin Level:80",
        Default = false,
        Callback = function(value)
            Mon = value
            if Mon then
    
            local tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)
            local tweenServiceClose, tweenInfoClose = game:GetService("TweenService"), TweenInfo.new(0.7, Enum.EasingStyle.Linear)
            local player = game:GetService("Players").LocalPlayer
            local char = player.Character
                
                local distance = (CFrame.new(-4489.6455078125, -339.67413330078125, -941.3670043945312).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                local speed = 300
                local time = distance / speed
                local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                    {CFrame = CFrame.new(-4489.6455078125, -339.67413330078125, -941.3670043945312)})
                Tw:Play()
                wait(5)
            
            while Mon do
                for i,v in pairs(game.workspace.Mobs:GetChildren()) do
                    if string.match(v.Name, MobName) and v:FindFirstChild("HumanoidRootPart") then
                        if v.HumanoidRootPart and v.HumanoidRootPart:IsA("BasePart") and v.HumanoidRootPart.Position.Z - char.HumanoidRootPart.Position.Z <= 5000 then

                            spawn(function()
                                BowAttack = true
                                while BowAttack and v.Humanoid and v.Humanoid.Health > 0 do wait()
                                    char.DualSwords.SwordScript.Activate:FireServer()
                                end
                                wait(30)
                                BowAttack = false
                                wait(5)
                                BowAttack = true
                            end)
                            

                            while Mon and v.Humanoid.Health > 0 do
                                wait(1)
                                tweenServiceClose:Create(player.Character.HumanoidRootPart, tweenInfoClose, {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,3)}):Play()
                            end
             
                            repeat wait() until v.Humanoid.Health <= 0
                            BowAttack = false
                            wait(1)
                            local distance = (v.HumanoidRootPart.CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = v.HumanoidRootPart.CFrame})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(-4461.2177734375, -339.6742248535156, -581.7333374023438).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(-4461.2177734375, -339.6742248535156, -581.7333374023438)})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(-4489.6455078125, -339.67413330078125, -941.3670043945312).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(-4489.6455078125, -339.67413330078125, -941.3670043945312)})
                            Tw:Play()
                            wait(2)
                        end
                    end
                end
                wait()
            end
            Mon = false
        end
    end
})


-----------------------------------------------------------------------------------------------------------------

---DualSwords Ruined Knight Level:85
local MobName = "RuinedKnight"
local BowAttack = false
local Mon = false

local RuinedKnightMob = true
Tab4:AddToggle({
    Name = "Ruined Knight Level:85",
    Default = false,
    Callback = function(value)
            Mon = value
            if Mon then
    
            local tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)
            local tweenServiceClose, tweenInfoClose = game:GetService("TweenService"), TweenInfo.new(0.7, Enum.EasingStyle.Linear)
            local player = game:GetService("Players").LocalPlayer
            local char = player.Character
                
            local distance = (CFrame.new(-3347.35595703125, -339.0802001953125, -911.8562622070312).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
            local speed = 300
            local time = distance / speed
            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                {CFrame = CFrame.new(-3347.35595703125, -339.0802001953125, -911.8562622070312)})
            Tw:Play()
            wait(5)
            
            while Mon do
                for i,v in pairs(game.workspace.Mobs:GetChildren()) do
                    if string.match(v.Name, MobName) and v:FindFirstChild("HumanoidRootPart") then
                        if v.HumanoidRootPart and v.HumanoidRootPart:IsA("BasePart") and v.HumanoidRootPart.Position.Z - char.HumanoidRootPart.Position.Z <= 5000 then

                            spawn(function()
                                BowAttack = true
                                while BowAttack and v.Humanoid and v.Humanoid.Health > 0 do wait()
                                    char.DualSwords.SwordScript.Activate:FireServer()
                                end
                                wait(30)
                                BowAttack = false
                                wait(5)
                                BowAttack = true
                            end)
                            

                            while Mon and v.Humanoid.Health > 0 do
                                wait(1)
                                tweenServiceClose:Create(player.Character.HumanoidRootPart, tweenInfoClose, {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,3)}):Play()
                            end
             
                            repeat wait() until v.Humanoid.Health <= 0
                            BowAttack = false
                            wait(1)
                            local distance = (v.HumanoidRootPart.CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = v.HumanoidRootPart.CFrame})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(-4461.2177734375, -339.6742248535156, -581.7333374023438).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(-4461.2177734375, -339.6742248535156, -581.7333374023438)})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(-3347.35595703125, -339.0802001953125, -911.8562622070312).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(-3347.35595703125, -339.0802001953125, -911.8562622070312)})
                            Tw:Play()
                            wait(2)
                        end
                    end
                end
                wait()
            end
            Mon = false
        end
    end
})
