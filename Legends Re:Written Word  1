Tab2:AddToggle({Name = "Bronz Ore",Default = false,Callback = function(value)
    Bronz = value
        if value then
            local tweenService = game:GetService("TweenService")
            local Character = game.Players.LocalPlayer.Character
            local HumanoidRootPart = Character.HumanoidRootPart

            local distance = (CFrame.new(-2762.74609375, -5.050119400024414, -734.9026489257812).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
            local speed = 300
            local time = distance / speed
            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                {CFrame = CFrame.new(-2762.74609375, -5.050119400024414, -734.9026489257812)})
            Tw:Play()
            Tw.Completed:Connect(function()
                TweenRunning = false
            end)

            Tw.Completed:Connect(function()
                wait(2)
                spawn(function()
                    while value and not TweenRunning do
                        HumanoidRootPart.CFrame = CFrame.new(-2806.59375, -5.050121307373047, -707.5731201171875)
                        wait(3)
                        HumanoidRootPart.CFrame = CFrame.new(-2781.3564453125, -5.050136566162109, -945.8433227539062)
                        wait(3)
                        HumanoidRootPart.CFrame = CFrame.new(-2928.009765625, 48.94988250732422, -1014.3327026367188)
                        wait(0.1)
                        HumanoidRootPart.CFrame = CFrame.new(-2935.64501953125, 48.940521240234375, -1165.3265380859375)
                        wait(0.1)
                        HumanoidRootPart.CFrame = CFrame.new(-2963.554443359375, 49.292938232421875, -1290.8770751953125)
                        wait(3)
                        HumanoidRootPart.CFrame = CFrame.new(-2972.076416015625, 49.475257873535156, -1310.5716552734375)
                        wait(3)
                        HumanoidRootPart.CFrame = CFrame.new(-2935.64501953125, 48.940521240234375, -1165.3265380859375)
                        wait(0.1)
                        HumanoidRootPart.CFrame = CFrame.new(-2928.009765625, 48.94988250732422, -1014.3327026367188)
                        wait(0.1)
                        HumanoidRootPart.CFrame = CFrame.new(-2765.198486328125, -5.055056571960449, -940.6439208984375)
                        wait(0.1)
                    end
                end)

                _G.loop = true
                while _G.loop do
                    wait()
                    game:GetService("Players").LocalPlayer.Character.PickAxe.SwordScript.Activate:FireServer()
                    wait(0.1)
                end
            end)
        else

            _G.loop = false
            TweenRunning = true
        end
end
})

