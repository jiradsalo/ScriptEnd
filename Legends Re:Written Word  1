            {CFrame = CFrame.new(-2938.394775390625, 48.940223693847656, -1075.271484375)})
            Tw:Play()
            wait(10)
            
            local Tw1 = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 130}),
