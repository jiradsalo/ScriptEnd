                                BowAttack = true
                                while BowAttack and v.Humanoid and v.Humanoid.Health > 0 do wait()
                                    char.DualSwords.SwordScript.Activate:FireServer()
                                end
                                wait(30)
                                BowAttack = false
                                wait(5)
                                BowAttack = true
                            end)
                            

                            while Mon and v.Humanoid.Health > 0 do
                                wait(1)
                                tweenServiceClose:Create(player.Character.HumanoidRootPart, tweenInfoClose, {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,3)}):Play()
                            end
             
                             repeat wait() until v.Humanoid.Health <= 0
                            BowAttack = false
                            wait(1)
                            local distance = (v.HumanoidRootPart.CFrame.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = v.HumanoidRootPart.CFrame})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(-1065.4139404296875, 42.86543655395508, -1835.20703125).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(-1065.4139404296875, 42.86543655395508, -1835.20703125)})
                            Tw:Play()
                            wait(4)
                            
                            local distance = (CFrame.new(-902.2942504882812, 155.3244171142578, -1599.96484375).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
                            local speed = 300
                            local time = distance / speed
                            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                                {CFrame = CFrame.new(-902.2942504882812, 155.3244171142578, -1599.96484375)})
                            Tw:Play()
                            wait(2)
                        end
                    end
                end
                wait()
            end
            Mon = false
        end
    end
