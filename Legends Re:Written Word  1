local MobName = "Nodryre"
local BowAttack = false
local Mon = false

    local NodryreMob = true
    Tab4:AddToggle({
        Name = "Nodryre Level:100",
        Default = false,
        Callback = function(value)
            Mon = value
            if Mon then
    
    
            local tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(1, Enum.EasingStyle.Linear)
