Tab2:AddToggle({Name = "Rune Ore",Default = false,Callback = function(value)
    Rune = value
        if value then
            local tweenService = game:GetService("TweenService")
            local Character = game.Players.LocalPlayer.Character
            local HumanoidRootPart = Character.HumanoidRootPart
        
            local distance = (CFrame.new(-3308.418701171875, 176.96286010742188, -810.0545043945312).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
            local speed = 300
            local time = distance / speed
            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0), 
                {CFrame = CFrame.new(-3308.418701171875, 176.96286010742188, -810.0545043945312)})
            Tw:Play()
