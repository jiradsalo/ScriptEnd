            local player = game:GetService("Players").LocalPlayer
            local char = player.Character
                
                local distance = (CFrame.new(231.00592041015625, 120.15098571777344, -203.12843322753906).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
