local Section = Tab4:AddSection({
	Name = "Bow Farm"
})


-------------------------------------------------------------------------------------------------------------------------------------------------------------------
---Boar King Level:40
local MobName = "BoarMan"
local BowAttack = true
local Mon = true

    Tab4:AddToggle({
        Name = "Boar King Level:40",
        Default = false,
        Callback = function(value)
            Mon = value
            if Mon then
    
                local tweenService = game:GetService("TweenService")
                local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear)
                local tweenServiceClose = game:GetService("TweenService")
                local tweenInfoClose = TweenInfo.new(0.7, Enum.EasingStyle.Linear)
                local player = game:GetService("Players").LocalPlayer
                local char = player.Character
                
            local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 130}),
            {CFrame = CFrame.new(-3855.776611328125, 598.949951171875, -856.896484375)})
            Tw:Play()
            wait(5)

                while Mon do
                for i,v in pairs(game.workspace.Mobs:GetChildren()) do
                if string.match(v.Name, MobName) and v:FindFirstChild("HumanoidRootPart") then
                    
                spawn(function()
                    while BowAttack do
                    char.Bow.SwordScript.Shoot:FireServer(v.HumanoidRootPart.Position, 21)
                    wait(0.1)
                    end
                end)
                    wait(5)
                    BowAttack = false
                    wait(5)
                    BowAttack = true
                end
            end
            wait()
        end
    end
    Mon = false
end
})


                            

