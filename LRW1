local FirstPage = MainUI.AddPage("Fram Boss")

local MON = "Regulus"
local Methodnow = 1
local Method = CFrame.new(0,0,3)
local DualSwordsEnabled = false
local LoopConnection
local TweenRunning = false

local function TeleportToMON()
    for i,v in pairs(game:GetService("Workspace").Mobs:GetChildren()) do
        if v.Name == MON then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * Method
            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
        end
    end
end

local function ToggleLoop(state)
    DualSwordsEnabled = state
    if DualSwordsEnabled and not TweenRunning then
        LoopConnection = game:GetService("RunService").Heartbeat:Connect(function()
            TeleportToMON()
            if DualSwordsEnabled then
                workspace.cgg250za.DualSwords.SwordScript.Activate:FireServer()
            end
            wait(0.1)
        end)
    else
        if LoopConnection then
            LoopConnection:Disconnect()
        end
    end
end

if currentOption == "Regulus Level:???" then
    local TweenService = game:GetService("TweenService")
    local targetPosition = CFrame.new(-2962.012451171875, -339.6742248535156, -378.2539367675781)
    local tweenInfo = TweenInfo.new(6, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0)
    local Tw = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, tweenInfo, {CFrame = targetPosition})
    TweenRunning = true
    Tw:Play()
    Tw.Completed:Connect(function()
        TweenRunning = false
        ToggleLoop(DualSwordsEnabled)
    end)
end

local DualSwordsEnabled = FirstPage.AddToggle("DualSwords", false, function(state)
    DualSwordsEnabled = state
    ToggleLoop(DualSwordsEnabled)
end)
--
---
----
-----
------
-------
--------
-------------------------------------------------  End Fram Boss  -----------------------------------------------


