local UILibrary = loadstring(game:HttpGet("https://pastebin.com/raw/V1ca2q9s"))()
local MainUI = UILibrary.Load("EndHub - Treasure Quest")

-------------------------------------------------  Star Players  -------------------------------------------------
--------
------
-----
----
---
--
local FirstPage = MainUI.AddPage("Players")


local InfJumpEnabled = true
local InfJumpEnabled = FirstPage.AddToggle("InfJump", false, function(state)
    InfJumpEnabled = state
    if InfJumpEnabled then
        game:GetService("UserInputService").JumpRequest:connect(function()
            if InfJumpEnabled then
                game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
            end
        end)
    else
        game:GetService("UserInputService").JumpRequest:disconnect()
    end
end)


local SpeedEnabled = true
local SpeedToggle = FirstPage.AddToggle("Speed = 80", false, function(state)
    SpeedEnabled = state
    if SpeedEnabled then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 80
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)


local SpeedEnabled = true
local SpeedToggle = FirstPage.AddToggle("Speed = 150", false, function(state)
    SpeedEnabled = state
    if SpeedEnabled then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 150
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)


local DashEnabled = true
local DashToggle = FirstPage.AddToggle("Fram Speed", false, function(state)
    toggleDash = state
    DashEnabled = state
    if DashEnabled then
        while DashEnabled do
            game:GetService("Players").LocalPlayer.PlayerGui.Parkour.Script.Dash:FireServer()
            wait(0.00000000000000000000000000000000000000000000000000001)
        end
    end
end)

--
