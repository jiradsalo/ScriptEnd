local Tab1 = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://12836323838",
	PremiumOnly = false
})
-------------------------------------------
local Tab2 = Window:MakeTab({
	Name = "Farm Ore",
	Icon = "rbxassetid://12836232943",
	PremiumOnly = false
})
-------------------------------------------
local Tab3 = Window:MakeTab({
	Name = "Artifac",
	Icon = "rbxassetid://12836267517",
	PremiumOnly = false
})
--------------------------------------------
local Tab4 = Window:MakeTab({
	Name = "Farm Boss",
	Icon = "rbxassetid://12836287616",
	PremiumOnly = false
})
---------------------------------------------
local Tab5 = Window:MakeTab({
	Name = "Oter",
	Icon = "rbxassetid://12836304245",
	PremiumOnly = false
})
