                HumanoidRootPart.CFrame = CFrame.new(-3236.67724609375, 259.9465026855469, -1422.78515625)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-2848.415771484375, 237.119384765625, -1318.3720703125)
                wait(3)
            end
        end)
        
     _G.loop = true
        while _G.loop do wait()
        game:GetService("Players").LocalPlayer.Character.PickAxe.SwordScript.Activate:FireServer()
        wait(0.1)
        end
    end)
end
end

FirstPage.AddToggle("Crystal Ore HP:140", false, function(enabled)
    ToggleFarmOre(enabled)
end)

-- Stardust Ore
local FarmOreEnabled = false
local TweenRunning = false

local function ToggleFarmOre(state)
    FarmOreEnabled = state
    if FarmOreEnabled and not TweenRunning then
        TweenRunning = true
        local TweenService = game:GetService("TweenService")
        local Character = game.Players.LocalPlayer.Character
        local HumanoidRootPart = Character.HumanoidRootPart
        
        local Tween1 = TweenService:Create(HumanoidRootPart, TweenInfo.new(6, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 200}),
            {CFrame = CFrame.new(-3580.855712890625, -339.0189208984375, -515.154541015625)})
        Tween1:Play()
        Tween1.Completed:Connect(function()
            TweenRunning = false
        end)

        Tween1.Completed:Connect(function()
        wait(2)
        spawn(function()
            while FarmOreEnabled and not TweenRunning do
                HumanoidRootPart.CFrame = CFrame.new(-3503.60595703125, -339.6742248535156, -459.7414245605469)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-3467.051025390625, -339.6742248535156, -469.66162109375)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-3495.773681640625, -339.6742248535156, -578.3414306640625)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-3186.53076171875, 421.57598876953125, -463.7036437988281)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-3155.93310546875, 421.57598876953125, -537.44482421875)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-3157.453125, 421.57598876953125, -723.3267822265625)
                wait(3)
            end
        end)
        
     _G.loop = true
        while _G.loop do wait()
        game:GetService("Players").LocalPlayer.Character.PickAxe.SwordScript.Activate:FireServer()
        wait(0.1)
        end
    end)
end
end

FirstPage.AddToggle("Stardust Ore HP:669", false, function(enabled)
    ToggleFarmOre(enabled)
end)
