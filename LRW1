local FirstPage = MainUI.AddPage("Fram Ore")


-- Bronz Ore
local FarmOreEnabled = false
local TweenRunning = false

local function ToggleFarmOre(state)
    FarmOreEnabled = state
    if FarmOreEnabled and not TweenRunning then
        TweenRunning = true
        local TweenService = game:GetService("TweenService")
        local Character = game.Players.LocalPlayer.Character
        local HumanoidRootPart = Character.HumanoidRootPart
        
        local Tween1 = TweenService:Create(HumanoidRootPart, TweenInfo.new(6, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 200}),
            {CFrame = CFrame.new(-2762.74609375, -5.050119400024414, -734.9026489257812)})
        Tween1:Play()
        Tween1.Completed:Connect(function()
            TweenRunning = false
        end)

        Tween1.Completed:Connect(function()
        wait(2)
        spawn(function()
            while FarmOreEnabled and not TweenRunning do
                HumanoidRootPart.CFrame = CFrame.new(-2807.071044921875, -5.0501203536987305, -707.8900146484375)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-2784.137939453125, -4.892110824584961, -948.5869140625)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-2964.209716796875, 49.29293441772461, -1290.026123046875)
                wait(3)
                HumanoidRootPart.CFrame = CFrame.new(-2972.51220703125, 49.292945861816406, -1310.7652587890625)
                wait(3)
            end
        end)
        
     _G.loop = true
        while _G.loop do wait()
        game:GetService("Players").LocalPlayer.Character.PickAxe.SwordScript.Activate:FireServer()
        wait(0.1)
        end
    end)
end
end

FirstPage.AddToggle("Bronze Ore Hp:5", false, function(enabled)
    ToggleFarmOre(enabled)
end)



