---Flame Dragon Level:105
local MobName = "Dragon"

local DragonMon = true
Tab4:AddToggle({
    Name = "Dragon Level:105",
    Default = false,
    Callback = function(value)
        DragonMon = value
        if DragonMon then

            local tweenService = game:GetService("TweenService")
            local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear)
            local tweenServiceClose = game:GetService("TweenService")
            local tweenInfoClose = TweenInfo.new(0.7, Enum.EasingStyle.Linear)
            local player = game:GetService("Players").LocalPlayer
            local char = player.Character
            
            local Tw1 = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(6, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 130}),
            {CFrame = CFrame.new(-2473.820068359375, -4.971416473388672, -515.5170288085938)})
            Tw1:Play()
            wait(3)
            
            if DragonMon then
            for i,v in pairs(game.workspace.Mobs:GetChildren()) do
                if string.match(v.Name, MobName) then
                    if v:FindFirstChild("HumanoidRootPart") then
                        
                        
                         _G.loop = true
                        while _G.loop do wait(1)
                        local Tw = tweenServiceClose:Create(player.Character.HumanoidRootPart, tweenInfoClose, {CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,50)})
                    
                        Tw:Play()
                        
                        
                    
                        _G.loop = true
                        while _G.loop do wait()
                            char.Bow.SwordScript.Shoot:FireServer(v.HumanoidRootPart.Position, 21)
                            char.Bow.SwordScript.Shoot:FireServer(v.HumanoidRootPart.Position, 21)
                        end
                        end
                    end
                end
            end
            end
        else
            _G.loop = false
        end
    end
})

