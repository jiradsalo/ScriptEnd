---Melioofus Level:200
local MobName = "Melioofus"

local MelioofusMon = true
Tab4:AddToggle({
    Name = "Melioofus Level:200",
    Default = false,
    Callback = function(value)
        MelioofusMon = value
        if MelioofusMon then

            local tweenService = game:GetService("TweenService")
            local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear)
            local tweenServiceClose = game:GetService("TweenService")
            local tweenInfoClose = TweenInfo.new(0.7, Enum.EasingStyle.Linear)
            local player = game:GetService("Players").LocalPlayer
            local char = player.Character
            
            local Tw1 = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(6, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 130}),
            {CFrame = CFrame.new(-505.3862609863281, 320.0796813964844, -306.233642578125)})
            Tw1:Play()
            wait(3)
            
            if MelioofusMon then
            for i,v in pairs(game.workspace.Mobs:GetChildren()) do
                if string.match(v.Name, MobName) then
                    if v:FindFirstChild("HumanoidRootPart") then
                        
                        local Tw = tweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(3, Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0, false, 0, {Speed = 130}),
                            {CFrame = CFrame.new(-827.4727172851562, 336.8792724609375, 158.16583251953125)})
                            Tw:Play()
                    
                            _G.loop = true
                            while _G.loop do wait()
                                char.Bow.SwordScript.Shoot:FireServer(v.HumanoidRootPart.Position, 21)
                                char.Bow.SwordScript.Shoot:FireServer(v.HumanoidRootPart.Position, 21)
                                
                            end
                        end
                    end
            end
            end
            else
                _G.loop = false
            end
        end
    })

